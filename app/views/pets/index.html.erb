<h1>Pets#index</h1>
<p>Find me in app/views/pets/index.html.erb</p>


<div class="container">
  <div class="row">
    <% @pets.each do |pet| %>
    <div class="col-xs-12 col-sm-6 col-md-3">

      <%= link_to "", pet_path(pet), class: 'card-link' %>
      <div class="card-description" class ="card-label">
        <%= pet.name %>
        <%= link_to 'Delete pet', pet_path(pet), method: :delete %>
      </div>
    </div>
  </div>
  <% end %>
      </div>
    </div>
  </div>
